{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "one-line": [
      true,
      "check-finally",
      "check-open-brace",
      "check-whitespace"
    ],
    "space-before-function-paren": false,
    "quotemark": [
      true,
      "single"
    ],
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ]
  },
  "rulesDirectory": []
}