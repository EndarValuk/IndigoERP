{
  "apps": [
    {
      "name": "indigo",
      "script": "npm",
      "args": "run buildAndStart",
      "watch": [
        "src"
      ],
      "ignore_watch": [
        "**/*.test.ts",
        "**/*.spec.ts",
        ".git",
        "node_modules"
      ],
      "watch_options": {
        "followSymlinks": false
      },
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}